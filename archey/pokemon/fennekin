${P_BALCK#    }██${P_RED#    }██${P_RED#    }██${P_a#    }██${P_YELLOW#    }██${P_BALCK#    }██  ${P_BALCK#    }██${P_YELLOW#    }██${P_YELLOW#    }██${P_a#    }██${P_a#    }██${P_RED#    }██${P_RED#    }██${P_BALCK#    }██        ${fieldlist[5]}
${P_BALCK#    }██${P_RED#    }██${P_RED#    }██${P_RED#    }██${P_b#    }██${P_GRAY#    }██${P_BALCK#    }██${P_YELLOW#    }██${P_YELLOW#    }██${P_a#    }██${P_RED#    }██${P_RED#    }██${P_RED#    }██${P_BALCK#    }██          ${fieldlist[6]}
  ${P_BALCK#    }██${P_a#    }██${P_GRAY#    }██${P_YELLOW#    }██${P_YELLOW#    }██${P_YELLOW#    }██${P_YELLOW#    }██${P_a#    }██${P_RED#    }██${P_RED#    }██${P_RED#    }██${P_RED#    }██${P_BALCK#    }██  ${P_BALCK#    }██      ${fieldlist[7]}
    ${P_BALCK#    }██${P_YELLOW#    }██${P_YELLOW#    }██${P_YELLOW#    }██${P_YELLOW#    }██${P_YELLOW#    }██${P_a#    }██${P_RED#    }██${P_RED#    }██${P_RED#    }██${P_GRAY#    }██${P_BALCK#    }██${P_BALCK#    }██${P_RED#    }██${P_BALCK#    }██    ${fieldlist[8]}
  ${P_BALCK#    }██${P_YELLOW#    }██${P_YELLOW#    }██${P_YELLOW#    }██${P_YELLOW#    }██${P_YELLOW#    }██${P_YELLOW#    }██${P_YELLOW#    }██${P_a#    }██${P_GRAY#    }██${P_GRAY#    }██${P_b#    }██${P_YELLOW#    }██${P_RED#    }██${P_RED#    }██${P_BALCK#    }██    ${fieldlist[9]}
  ${P_BALCK#    }██${P_YELLOW#    }██${P_YELLOW#    }██${P_YELLOW#    }██${P_GRAY#    }██${P_GRAY#    }██${P_YELLOW#    }██${P_YELLOW#    }██${P_WRITE#    }██${P_WRITE#    }██${P_GRAY#    }██${P_YELLOW#    }██${P_YELLOW#    }██${P_YELLOW#    }██${P_YELLOW#    }██${P_BALCK#    }██    ${fieldlist[10]}
