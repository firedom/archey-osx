  P_BALCK=$(tput       setaf 232 2>/dev/null)
  P_YELLOW=$(tput       setaf 226 2>/dev/null)
  P_ORANGE=$(tput       setaf 214 2>/dev/null)
  P_RED=$(tput       setaf 196 2>/dev/null)
  P_GRAY=$(tput       setaf 240 2>/dev/null)
  P_WRITE=$(tput  setaf 231 2>/dev/null)
  P_a=$(tput       setaf 94 2>/dev/null)
  P_b=$(tput       setaf 136 2>/dev/null)
  P_c=$(tput       setaf 251 2>/dev/null)

